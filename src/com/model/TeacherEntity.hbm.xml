<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.model.TeacherEntity" table="Teacher" schema="StuDB">
        <id name="tid">
            <column name="TID" sql-type="int(11)"/>
        </id>
        <property name="tUserName">
            <column name="TUserName" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="tName">
            <column name="TName" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="tPassword">
            <column name="TPassword" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="ttell">
            <column name="Ttell" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>