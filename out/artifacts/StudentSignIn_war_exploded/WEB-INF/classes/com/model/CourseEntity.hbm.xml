<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.model.CourseEntity" table="Course" schema="StuDB">
        <id name="cid">
            <column name="CID" sql-type="int(11)"/>
        </id>
        <property name="cName">
            <column name="CName" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="tUserName">
            <column name="TUserName" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="cStart">
            <column name="CStart" sql-type="time"/>
        </property>
        <property name="cEnd">
            <column name="CEnd" sql-type="time"/>
        </property>
        <property name="cno">
            <column name="Cno" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>